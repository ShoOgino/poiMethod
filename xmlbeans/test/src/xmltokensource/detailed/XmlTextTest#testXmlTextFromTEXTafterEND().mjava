    public void testXmlTextFromTEXTafterEND() throws Exception {
        m_xo = XmlObject.Factory.parse("<foo><bar> text </bar> ws \\r\\n </foo>");
        m_xc = m_xo.newCursor();
        toNextTokenOfType(m_xc, TokenType.TEXT);
        toNextTokenOfType(m_xc, TokenType.TEXT);
        assertEquals(" ws \\r\\n ", m_xc.getChars());
    }

