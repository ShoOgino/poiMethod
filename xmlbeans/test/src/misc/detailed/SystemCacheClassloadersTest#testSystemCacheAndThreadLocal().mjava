    public static void testSystemCacheAndThreadLocal()
    {
        Thread testThread = new SystemCacheThread("SchemTypeLoader Test Thread");

        try {
            testThread.start();
        }
        catch (Exception e) {
            e.printStackTrace();
        }

        finally {
            try {
                testThread.join();
            }
            catch (InterruptedException ie) {
                ie.printStackTrace();
            }
        }

    }

