    public void testSetValueNull() {
        ((Attr) m_node).setValue("foo");
        String newVal = "";
        ((Attr) m_node).setValue(newVal);
        assertEquals(true, ((Attr) m_node).hasChildNodes());

        newVal = null;
        ((Attr) m_node).setValue(newVal);
        assertEquals(true, ((Attr) m_node).hasChildNodes());
    }

