    public static boolean compareNodeTreeValue(NodeList lst1, NodeList lst2)
    {
        if (lst1.getLength() != lst2.getLength())
            return false;
        boolean result = true;
        for (int i = 0; i < lst1.getLength(); i++)
        {
            result = result && compareNodesDeep(lst1.item(i),
                lst2.item(i));

        }
        return result;
    }

