    //equates "" to null
    private static boolean compareNull(String pre1, String pre2)
    {
        if (pre1 == null && pre2 == null) return true;
        if (pre1 == null && pre2.equals("")) return true;
        if (pre1.equals("") && pre2 == null)
            return true;
        else
            return pre1.equals(pre2);


    }

