    private static void dumpErrors(List errors, PrintWriter out)
    {
        // Display the errors
        for (int i = 0; i < errors.size(); i++)
        {
            XmlError error = (XmlError) errors.get(i);
            if (error.getSeverity() == XmlError.SEVERITY_ERROR)
                out.println(error.toString());
        }
    }

