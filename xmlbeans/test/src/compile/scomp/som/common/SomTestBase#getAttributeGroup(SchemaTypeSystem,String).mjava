    public boolean getAttributeGroup(SchemaTypeSystem sts,
                                     String sAttrGrpLocalName)
    {
        SchemaAttributeGroup attrGp = sts.findAttributeGroup(new QName(sAttrGrpLocalName));
        if (attrGp == null)
        {
            return false;
        }
        else
        {
            return true;
        }
    }

