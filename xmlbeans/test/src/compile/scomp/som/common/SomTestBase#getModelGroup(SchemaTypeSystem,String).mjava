    public boolean getModelGroup(SchemaTypeSystem sts,
                                 String sModelGrpLocalName)
    {
        SchemaModelGroup.Ref modelGp = sts.findModelGroupRef(new QName(sModelGrpLocalName));
        if (modelGp == null)
        {
            return false;
        }
        else
        {
            return true;
        }
    }

