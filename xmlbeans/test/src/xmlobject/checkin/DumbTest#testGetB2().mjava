    public void testGetB2()
            throws Exception {
        RootDocument rootDoc = (RootDocument) XmlObject.Factory
                .parse(instance);
        Root root = rootDoc.getRoot();

        System.out.println("root.xmlText() = " + root.xmlText());

        assertTrue("bar:b attribute != 4", root.getB2().intValue() == 4);
    }

