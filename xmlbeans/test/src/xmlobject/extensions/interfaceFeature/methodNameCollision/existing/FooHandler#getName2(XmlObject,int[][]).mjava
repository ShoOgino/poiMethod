    public static String getName2(XmlObject xo, int[][] ints){
        StringBuffer buf = new StringBuffer();
        buf.append("Name2: ");
        for (int i = 0; i < ints.length; i++)
        {
            buf.append("[");
            for (int j = 0; j < ints[i].length; j++)
            {
                buf.append(ints[i][j]);
                if (j != ints[i].length)
                    buf.append(", ");
            }
            buf.append("]");
            if (i != ints.length)
                buf.append(", ");
        }
        return buf.toString();
     }

