    private void setTextValue() {
        XmlCursor c = getCursor();

        if (c.isFinish() || c.isNamespace() || c.isText())
            return;

        StringBuffer sb = new StringBuffer();

        for (int i = rnd(10); i >= 0; i--)
            sb.append(_chars[rnd(_chars.length)]);

        c.setTextValue(sb.toString());
    }

