    private QName getQName() {
        QName name = null;

        switch (rnd(3)) {
            case 0:
                name = XmlBeans.getQName("foo.com", "foo");
                break;
            case 1:
                name = XmlBeans.getQName("bar.com", "bar");
                break;
            case 2:
                name = XmlBeans.getQName("moo");
                break;
        }

        return name;
    }

