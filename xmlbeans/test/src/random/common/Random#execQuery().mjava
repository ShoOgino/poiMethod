    private void execQuery() {
        if (_noquery)
            return;

        if (rnd(20) > 0)
            return;

        QName name = getQName();

        String query =
                "declare namespace xxx='" + name.getNamespaceURI() + "' " +
                ".//xxx:" + name.getLocalPart();

        XmlObject x = getCursor().execQuery(query).getObject();

        if (rnd(3) == 0)
            _docs[rnd(_docs.length)] = x;
    }

