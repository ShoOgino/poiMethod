    private void changeType() {
        XmlObject o, n;

        try {
            o = findObject();
            SchemaType type = findObject().schemaType();
            n = o.changeType(type);
        }
        catch (IllegalArgumentException e) {
            return;
        }
        catch (IllegalStateException e) {
            return;
        }

        for (int i = 0; i < _docs.length; i++) {
            if (o == _docs[i]) {
                _docs[i] = n;
                break;
            }
        }
    }

