                "elementFormDefault=\"qualified\" targetNamespace=\"typeTests\"" +
                " xmlns:xs=\"http://www.w3.org/2001/XMLSchema\" >" +
                "<xs:element name=\"a\" type=\"xs:" + type + "\"" +
                " xmlns:xs=\"http://www.w3.org/2001/XMLSchema\" />" +
                "</xs:schema>";
    }

