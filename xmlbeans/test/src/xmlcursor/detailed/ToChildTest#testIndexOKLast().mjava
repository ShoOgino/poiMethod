     public void testIndexOKLast()throws Exception{
	String sExpectedValue="<char>zap<dar>wap</dar><ear>yap</ear></char>";
	m_xc = XmlObject.Factory.parse(sDoc).newCursor();
	m_xc.toFirstChild();
	assertEquals(true,m_xc.toChild(nChildCount-1));
