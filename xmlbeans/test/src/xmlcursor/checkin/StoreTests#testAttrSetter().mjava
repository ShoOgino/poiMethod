    public void testAttrSetter ( )
        throws Exception
    {
        XmlObject x = XmlObject.Factory.parse( "<foo/>" );
        XmlCursor c = x.newCursor();
        c.toNextToken();
        c.setAttributeText( new QName( null, "x" ), "hardehar" );
        Assert.assertTrue( x.xmlText().equals( "<foo x=\"hardehar\"/>" ) );
    }

