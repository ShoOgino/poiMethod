    {
        doTestSaxSaver( "<a xmlns='nnn'></a>" );
        doTestSaxSaver( "<a x='y'><!---->x<b/><c p='q'>z</c></a>" );
        doTestSaxSaver( "<a x='y'><!----><b>moo</b><c p='q'></c></a>" );
