    public void testSetName( )
        throws Exception
    {
        XmlObject x = XmlObject.Factory.parse( "<foo x='a'/>" );
        XmlCursor c = x.newCursor();
        c.toNextToken();
        c.setName( new QName( "bar" ) );
        c.toNextToken();
        c.setName( new QName( "y" ) );

        Assert.assertTrue( x.xmlText().equals( "<bar y=\"a\"/>" ) );
    }

