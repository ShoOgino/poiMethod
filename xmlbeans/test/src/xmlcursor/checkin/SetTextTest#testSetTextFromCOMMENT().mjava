    public void testSetTextFromCOMMENT() throws Exception {
        m_xo = XmlObject.Factory.parse(Common.XML_FOO_COMMENT);
        m_xc = m_xo.newCursor();
        toNextTokenOfType(m_xc, TokenType.COMMENT);
        m_xc.setTextValue("fred");
        assertEquals("fred", m_xc.getTextValue());
    }

