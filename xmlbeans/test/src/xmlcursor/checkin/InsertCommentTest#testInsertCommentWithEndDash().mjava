    public void testInsertCommentWithEndDash() throws Exception {
        m_xo = XmlObject.Factory.parse(Common.XML_FOO_TEXT);
        m_xc = m_xo.newCursor();
        toNextTokenOfType(m_xc, TokenType.TEXT);
        m_xc.insertComment(" -");
        m_xc.toStartDoc();
        assertEquals("<foo><!--  -->text</foo>", m_xc.xmlText());
    }

