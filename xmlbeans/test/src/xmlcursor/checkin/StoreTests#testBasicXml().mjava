    public void testBasicXml( )
        throws Exception
    {
        XmlCursor c = XmlObject.Factory.parse(Common.XML_ATTR_TEXT, null).newCursor();

        int n = 0;
        
        for ( ; ; )
        {
            TokenType t = c.toNextToken();

            n++;
            
            if (t == TokenType.NONE)
                break;
        }
        
        Assert.assertTrue( n == 6 );
    }

