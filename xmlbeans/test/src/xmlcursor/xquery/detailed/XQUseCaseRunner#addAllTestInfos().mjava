    public void addAllTestInfos()
    {
        UseCaseTest testCase;
        firstCall = true;

        try
        {
            findZip();
            UseCaseTest.pathToZip = pathToZip;
            while ((testCase = loadUseCaseTest()) != null)
            {
                addTest(testCase);
            }
        }
        catch (Exception e)
        {
            e.printStackTrace();
            throw new RuntimeException(e);
        }


    }

