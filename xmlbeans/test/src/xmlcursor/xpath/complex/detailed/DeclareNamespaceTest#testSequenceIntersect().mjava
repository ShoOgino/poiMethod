    public void testSequenceIntersect()
        throws Exception
    {
        XmlCursor o = XmlObject.Factory.parse("<a><b>1</b>1</a>").newCursor();
        o.selectPath("//b intersect //b");
        assertEquals(1, o.getSelectionCount());
        o.toNextSelection();
        assertEquals("<b>1</b>", o.xmlText());
    }

