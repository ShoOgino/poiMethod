            XmlObject.Factory.parse(
                "<DDD><EEE/><DDD/><CCC/><FFF/><FFF><GGG/></FFF></DDD>"),
            XmlObject.Factory.parse("<EEE/>"),
        };
        String ex2Simple = "//GGG/ancestor-or-self::*";
