    public void startElement(String uri,
                             String localName,
                             String qName,
                             Attributes attributes) throws SAXException
    {

      if(null != attributes)
      {
        _buff.append(SES);
        _buff.append(qName);
        _buff.append(attributes.getQName(0));
        _buff.append(EQ);
        _buff.append(attributes.getValue(0));
        _buff.append(EE);
      }
      else
      {
        _buff.append(SES);
        _buff.append(qName);
        _buff.append(EE);
      }

    }

