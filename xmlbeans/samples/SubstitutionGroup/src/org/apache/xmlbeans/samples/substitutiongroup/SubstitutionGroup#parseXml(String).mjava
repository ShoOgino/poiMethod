    public PurchaseOrderDocument parseXml(String file)
    {
        File xmlfile = new File(file);
        PurchaseOrderDocument poDoc = null;

        try 
        {
            poDoc = PurchaseOrderDocument.Factory.parse(xmlfile);
        }
        catch(XmlException e){
            e.printStackTrace();
        }
        catch(IOException e){
            e.printStackTrace();
        }
        return poDoc;
    }

