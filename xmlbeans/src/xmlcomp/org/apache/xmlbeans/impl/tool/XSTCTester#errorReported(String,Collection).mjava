    public static boolean errorReported(String errorCode, Collection set)
    {
        if (errorCode == null || set == null || set.size() == 0)
            return false;

        for (Iterator i = set.iterator(); i.hasNext(); )
        {
            if (errorCode.equals(((XmlError)i.next()).getErrorCode()))
                return true;
        }

        return false;
    }

