    void dumpAnnotation()
    {
        if (!atLeast(2, 19, 0))
            return; // no annotations in this version of the file

        int n = readInt();
        if (n == -1)
            return; // no annotation present
        emit("Annotation");
        boolean empty = true;
        indent();
        if (n > 0)
        {
            emit("Attributes (" + n + "):");
            indent();
            for (int i = 0; i < n; i++)
            {
                if (atLeast(2, 24, 0))
                    emit("Name: " + qnameString(readQName()) +
                        ", Value: " + readString() +
                        ", ValueURI: " + readString());
                else
                    emit("Name: " + qnameString(readQName()) +
                        ", Value: " + readString());
            }
            outdent();
            empty = false;
        }

        n = readInt();
        if (n > 0)
        {
            emit("Documentation elements (" + n + "):");
            indent();
            for (int i = 0; i < n; i++)
                emit(readString());
            outdent();
            empty = false;
        }

        n = readInt();
        if (n > 0)
        {
            emit("Appinfo elements (" + n + "):");
            indent();
            for (int i = 0; i < n; i++)
                emit(readString());
            outdent();
            empty = false;
        }
        if (empty)
            emit("<empty>");
        outdent();
    }

