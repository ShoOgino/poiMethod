    public File[] filesEndingWith(String ext)
    {
        List result = new ArrayList();
        for (Iterator i = getFileList().iterator(); i.hasNext(); )
        {
            File f = (File)i.next();
            if (f.getName().endsWith(ext) && !looksLikeURL(f.getPath()))
                result.add(f);
        }
        return (File[])result.toArray(EMPTY_FILEARRAY);
    }

