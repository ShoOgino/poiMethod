    public boolean toChild(QName name, int index) {
        if (preCheck()) {
            _cur._locale.enter();
            try {
                return _toChild(name, index);
            } finally {
                _cur._locale.exit();
            }
        } else synchronized (_cur._locale) {
            _cur._locale.enter();
            try {
                return _toChild(name, index);
            } finally {
                _cur._locale.exit();
            }
        }
    }

