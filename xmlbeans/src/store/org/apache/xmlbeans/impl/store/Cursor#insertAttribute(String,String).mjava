        if (preCheck()) {
            _cur._locale.enter();
            try {
                _insertAttribute(localName, uri);
            } finally {
                _cur._locale.exit();
            }
        } else synchronized (_cur._locale) {
            _cur._locale.enter();
            try {
                _insertAttribute(localName, uri);
            } finally {
                _cur._locale.exit();
            }
        }
    }

