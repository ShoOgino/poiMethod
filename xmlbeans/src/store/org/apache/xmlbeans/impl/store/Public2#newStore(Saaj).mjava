    public static XmlCursor newStore ( Saaj saaj )
    {
        Locale l = newLocale( saaj );

        if (l.noSync())         { l.enter(); try { return _newStore( l ); } finally { l.exit(); } }
        else synchronized ( l ) { l.enter(); try { return _newStore( l ); } finally { l.exit(); } }
    }

