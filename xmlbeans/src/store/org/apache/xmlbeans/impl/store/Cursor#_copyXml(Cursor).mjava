    public boolean _copyXml(Cursor to) {
        to.checkInsertionValidity(_cur);

        assert _cur.isText() || _cur.isNode();

        Cur c = to.tempCur();

        if (_cur.isText())
            to._cur.insertChars(_cur.getChars(-1), _cur._offSrc, _cur._cchSrc);
        else
            _cur.copyNode(to._cur);

        to._cur.moveToCur(c);

        c.release();

        return true;
    }

