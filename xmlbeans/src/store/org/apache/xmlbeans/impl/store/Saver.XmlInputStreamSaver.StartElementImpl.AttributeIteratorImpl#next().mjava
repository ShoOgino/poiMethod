                public Attribute next ( )
                {
                    synchronized (monitor())
                    {
                        checkVersion();

                        AttributeImpl attr = null;

                        if (_attributes != null)
                        {
                            attr = _attributes;
                            _attributes = attr._next;
                        }
                        else if (_namespaces != null)
                        {
                            attr = _namespaces;
                            _namespaces = attr._next;
                        }

                        return attr;
                    }
                }

