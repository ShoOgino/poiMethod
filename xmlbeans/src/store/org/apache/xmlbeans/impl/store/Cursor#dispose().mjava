    public void dispose() {
        if (_cur != null) {
            Locale l = _cur._locale;
            if (preCheck()) {
                l.enter();
                try {
                    _dispose();
                } finally {
                    l.exit();
                }
            } else synchronized (l) {
                l.enter();
                try {
                    _dispose();
                } finally {
                    l.exit();
                }
            }
        }
    }

