    public void insertNamespace(String prefix, String namespace) {
        if (preCheck()) {
            _cur._locale.enter();
            try {
                _insertNamespace(prefix, namespace);
            } finally {
                _cur._locale.exit();
            }
        } else synchronized (_cur._locale) {
            _cur._locale.enter();
            try {
                _insertNamespace(prefix, namespace);
            } finally {
                _cur._locale.exit();
            }
        }
    }

