    public void getAllBookmarkRefs(Collection listToFill) {
        if (preCheck()) {
            _cur._locale.enter();
            try {
                _getAllBookmarkRefs(listToFill);
            } finally {
                _cur._locale.exit();
            }
        } else synchronized (_cur._locale) {
            _cur._locale.enter();
            try {
                _getAllBookmarkRefs(listToFill);
            } finally {
                _cur._locale.exit();
            }
        }
    }

