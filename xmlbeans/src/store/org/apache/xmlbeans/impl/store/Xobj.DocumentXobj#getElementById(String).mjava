        public Element getElementById ( String elementId ) {
            if ( _idToElement == null )return null;
            Xobj o = (Xobj) _idToElement.get(elementId);
            if (o == null) return null;
            if (!isInSameTree(o))
            {
                _idToElement.remove(elementId);
            }
            return (Element)o;
        }

