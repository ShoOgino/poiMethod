        protected void procInst ( String target, String value )
        {
            if (!_stripProcinsts)
            {
                Xobj x = new Xobj.ProcInstXobj( _locale, target );

                start( x );
                text( value, 0, value.length() );
                end();

                _lastXobj = x;
                _lastPos  = 0;
            }
            _stripLeft = true;
        }

