        void moveTo ( Xobj x, int p )
        {
            assert isOnList( _xobj._bookmarks );

            if (_xobj != x)
            {
                _xobj._bookmarks = listRemove( _xobj._bookmarks );
                x._bookmarks = listInsert( x._bookmarks );

                _xobj = x;
            }

            _pos = p;
        }

