    void copyNode ( Cur to )
    {
        assert to != null;
        assert isNode();

        Xobj copy = _xobj.copyNode( to._locale );

        // TODO - in the moveNode case, I would not have to walk the tree for cursors ... optimize

        if (to.isPositioned())
            Cur.moveNode( copy, to );
        else
            to.moveTo( copy );
    }

