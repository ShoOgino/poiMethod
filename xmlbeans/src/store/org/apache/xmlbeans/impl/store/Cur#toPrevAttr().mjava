    boolean toPrevAttr ( )
    {
        if (isAttr())
        {
            if (_xobj._prevSibling == null)
                moveTo( _xobj.ensureParent() );
            else
                moveTo( _xobj._prevSibling );

            return true;
        }

        prev();

        if (!isContainer())
        {
            next();
            return false;
        }

        return toLastAttr();
    }

