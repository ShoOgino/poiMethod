        private void emitTextValue(SaveCur c) {
            c.push();
            c.next();

            if (c.isText()) {
                _nodeCur.next();
                _nodeCur.insertChars(c.getChars(), c._offSrc, c._cchSrc);
                _nodeCur.toParent();
            }

            c.pop();
        }

