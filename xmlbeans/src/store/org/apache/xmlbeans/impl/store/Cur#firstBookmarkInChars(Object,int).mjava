    int firstBookmarkInChars ( Object key, int cch )
    {
        assert isNormal();
        assert key != null;
        assert cch > 0;
        assert cch <= cchRight();

        int d = -1;

        if (isText())
        {
            for ( Bookmark b = _xobj._bookmarks ; b != null ; b = b._next )
                if (b._key == key && inChars( b, cch, false ))
                    d = (d == -1 || b._pos - _pos < d) ? b._pos - _pos : d;
        }

        return d;
    }

