    Dom getDom ( )
    {
        assert isNormal();
        assert isPositioned();

        if (isText())
        {
            int cch = cchLeft();

            for ( CharNode cn = getCharNodes() ; ; cn = cn._next )
                if ((cch -= cn._cch) < 0)
                    return cn;
        }

        return _xobj.getDom();
    }

