    public void insertAttribute(QName name) {
        if (preCheck()) {
            _cur._locale.enter();
            try {
                _insertAttribute(name);
            } finally {
                _cur._locale.exit();
            }
        } else synchronized (_cur._locale) {
            _cur._locale.enter();
            try {
                _insertAttribute(name);
            } finally {
                _cur._locale.exit();
            }
        }
    }

