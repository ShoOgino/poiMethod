    int firstBookmarkInCharsLeft ( Object key, int cch )
    {
        assert isNormal();
        assert key != null;
        assert cch > 0;
        assert cch <= cchLeft();

        int d = -1;

        if (cchLeft() > 0)
        {
            Xobj x = getDenormal();
            int  p = _posTemp - cch;

            for ( Bookmark b = x._bookmarks ; b != null ; b = b._next )
                if (b._key == key && x.inChars( p, b._xobj, b._pos, cch, false ))
                    d = (d == -1 || b._pos - p < d) ? b._pos - p : d;
        }

        return d;
    }

