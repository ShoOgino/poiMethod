    public void insertElement(QName name) {
        if (preCheck()) {
            _cur._locale.enter();
            try {
                _insertElement(name);
            } finally {
                _cur._locale.exit();
            }
        } else synchronized (_cur._locale) {
            _cur._locale.enter();
            try {
                _insertElement(name);
            } finally {
                _cur._locale.exit();
            }
        }
    }

