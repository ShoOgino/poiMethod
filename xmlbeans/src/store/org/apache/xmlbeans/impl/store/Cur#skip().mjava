    boolean skip ( )
    {
        assert isNode();

        if (_xobj.isRoot())
            return false;

        if (_xobj.isAttr())
        {
            if (_xobj._nextSibling == null || !_xobj._nextSibling.isAttr())
                return false;

            moveTo( _xobj._nextSibling, 0 );
        }
        else
            moveTo( getNormal( _xobj, _xobj.posAfter() ), _posTemp );

        return true;
    }

