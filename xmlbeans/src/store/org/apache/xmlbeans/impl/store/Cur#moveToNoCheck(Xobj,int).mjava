    void moveToNoCheck ( Xobj x, int p )
    {
        if (_state == EMBEDDED && x != _xobj)
        {
            _xobj._embedded = listRemove( _xobj._embedded );
            _locale._registered = listInsert( _locale._registered );
            _state = REGISTERED;
        }

        _xobj = x;
        _pos = p;
    }

