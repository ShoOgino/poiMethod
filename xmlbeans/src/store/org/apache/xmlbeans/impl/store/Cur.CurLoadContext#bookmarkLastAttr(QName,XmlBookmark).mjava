        protected void bookmarkLastAttr ( QName attrName, XmlBookmark bm )
        {
            if (_lastPos == 0 && _lastXobj.isAttr())
            {
                assert _lastXobj._parent != null;

                Xobj a = _lastXobj._parent.getAttr( attrName );

                if (a != null)
                    a.setBookmark( 0, bm.getKey(), bm );
            }
        }

