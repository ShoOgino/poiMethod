        public Table(StringEnumAbstractBase[] array)
        {
            _map = new HashMap(array.length);
            _list = new ArrayList(array.length + 1);
            for (int i = 0; i < array.length; i++)
            {
                _map.put(array[i].toString(), array[i]);
                int j = array[i].intValue();
                while (_list.size() <= j)
                    _list.add(null);
                _list.set(j, array[i]);
            }
        }

