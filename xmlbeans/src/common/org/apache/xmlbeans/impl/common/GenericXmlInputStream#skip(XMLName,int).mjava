    public boolean skip ( XMLName name, int eventType ) throws XMLStreamException
    {
        ensureInit();

        for ( ; _nextEvent != null ; next() )
        {
            if (_nextEvent.getType() == eventType &&
                  _nextEvent.hasName() &&
                    _nextEvent.getName().equals( name ))
            {
                return true;
            }
        }

        return false;
    }

