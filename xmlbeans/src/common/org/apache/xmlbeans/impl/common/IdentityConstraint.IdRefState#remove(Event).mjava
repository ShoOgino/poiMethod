        void remove(Event e) 
        { 
            // Validate each ref has a corresponding ID
            for (Iterator it = _values.iterator() ; it.hasNext() ; )
            {
                Object o = it.next();
                if (! _ids._values.contains(o))
                {
                    // KHK: cvc-id.1
                    emitError(e, "ID not found for IDRef value '" + o + "'");
                }
            }
        }

