    protected void set_text(String s)
    {
        int v;

        try { v = XsTypeConverter.lexInt(s); }
        catch (Exception e) { throw new XmlValueOutOfRangeException(); }
        
        if (_validateOnSet())
        {
            validateValue(v, _schemaType, _voorVc);
            validateLexical(s, _schemaType, _voorVc);
        }

        super.set_int(v);
    }

