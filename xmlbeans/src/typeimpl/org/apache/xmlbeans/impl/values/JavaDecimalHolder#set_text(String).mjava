    protected void set_text(String s)
    {
        if (_validateOnSet())
            validateLexical(s, _voorVc);

        try {
            set_BigDecimal(new BigDecimal(s));
        }
        catch (NumberFormatException e)
        {
            _voorVc.invalid(XmlErrorCodes.DECIMAL, new Object[] { s });
        }
    }

