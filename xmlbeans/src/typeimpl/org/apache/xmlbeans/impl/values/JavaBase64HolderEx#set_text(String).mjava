    protected void set_text(String s)
    {
        final byte[] v;

        if (_validateOnSet())
            v = validateLexical(s, schemaType(), _voorVc);
        else
            v = lex(s, _voorVc);

        if (v != null && _validateOnSet())
            validateValue(v, schemaType(), XmlObjectBase._voorVc);
        
        super.set_ByteArray(v);
    }

