    public static String getQNameString(String uri,
                                        String localpart,
                                        String prefix)
    {
        if (prefix != null &&
            uri != null &&
            uri.length() > 0 &&
            prefix.length() > 0) {
            return (prefix + NAMESPACE_SEP + localpart);
        } else {
            return localpart;
        }
    }

