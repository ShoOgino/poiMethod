    static BigInteger extractMaxOccurs(AllNNI allNNI)
    {
        if (allNNI == null)
            return BigInteger.ONE;

        if (allNNI.instanceType().getPrimitiveType().getBuiltinTypeCode() == SchemaType.BTC_DECIMAL)
            return ((XmlInteger)allNNI).getBigIntegerValue();
        else
            return null;
    }

