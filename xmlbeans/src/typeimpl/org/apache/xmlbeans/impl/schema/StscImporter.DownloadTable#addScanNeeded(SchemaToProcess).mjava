        private SchemaToProcess addScanNeeded(SchemaToProcess stp)
        {
            if (!scannedAlready.containsKey(stp))
            {
                scannedAlready.put(stp, stp);
                scanNeeded.add(stp);
                return stp;
            }
            else
                return (SchemaToProcess) scannedAlready.get(stp);
        }

