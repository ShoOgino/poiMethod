    private static String printParticles(SchemaParticle[] parts, int start, int end)
    {
        StringBuffer buf = new StringBuffer(parts.length * 30);
        for (int i = start; i < end; )
        {
            buf.append(printParticle(parts[i]));
            if (++i != end)
                buf.append(", ");
        }
        return buf.toString();
    }

