        List readQNameRefMapAsList(List names)
        {
            int size = readShort();
            List result = new ArrayList(size);
            for (int i = 0; i < size; i++)
            {
                QName name = readQName();
                SchemaComponent.Ref obj = readHandle();
                result.add(obj);
                names.add(name);
            }
            return result;
        }

