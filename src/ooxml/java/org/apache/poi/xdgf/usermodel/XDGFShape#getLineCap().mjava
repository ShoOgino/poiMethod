    @Override
    public Integer getLineCap() {

        Integer lineCap = super.getLineCap();
        if (lineCap != null)
            return lineCap;

        // get from master
        if (_masterShape != null) {
            return _masterShape.getLineCap();
        }

        // get default
        XDGFStyleSheet style = _document.getDefaultLineStyle();
        if (style != null)
            return style.getLineCap();

        return null;
    }

