    @Override
	public Double getLineWeight() {
		
    	Double lineWeight = super.getLineWeight();
		if (lineWeight != null)
			return lineWeight;
		
		// get from master
		if (_masterShape != null) {
			return _masterShape.getLineWeight();
		}
		
		// get default
		XDGFStyleSheet style = _document.getDefaultLineStyle();
		if (style != null)
			return style.getLineWeight();
					
		return null;
	}

