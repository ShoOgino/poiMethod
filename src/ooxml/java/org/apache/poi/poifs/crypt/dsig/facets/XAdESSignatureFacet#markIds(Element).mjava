    private void markIds(Element element) {
        if (element != null) {
            Attr att = element.getAttributeNode("Id");
            if (att != null) {
                element.setIdAttributeNode(att, true);
            }
            NodeList nl = element.getChildNodes();
            for (int i = 0; i < nl.getLength(); i++) {
                markIds(nl.item(i));
            }
        }
    }

