    public final String toStringWithParameters() {
        StringBuffer retVal = new StringBuffer();
        retVal.append(toString());
        
        for (String key : parameters.keySet()) {
           retVal.append(";");
           retVal.append(key);
           retVal.append("=");
           retVal.append(parameters.get(key));
        }
        return retVal.toString();
    }

