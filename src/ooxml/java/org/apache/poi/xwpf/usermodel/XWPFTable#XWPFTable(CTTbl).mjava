    public XWPFTable(CTTbl table)
    {
        for(CTRow row : table.getTrArray())
        {
            for(CTTc cell : row.getTcArray())
            {
                for(CTP ctp : cell.getPArray())
                {
                    XWPFParagraph p = new XWPFParagraph(ctp);
                    this.text.append(p.getText()+"\t");
                }
            }
            this.text.append("\n");
        }
    }

