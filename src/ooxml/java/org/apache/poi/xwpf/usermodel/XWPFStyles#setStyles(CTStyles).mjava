    /**
     * Sets the ctStyles
     * @param styles
     */
    @SuppressWarnings("deprecation")
    public void setStyles(CTStyles styles) {
        ctStyles = styles;

        // Build up all the style objects
        for(CTStyle style : ctStyles.getStyleArray()) {
            listStyle.add(new XWPFStyle(style, this));
        }
    }

