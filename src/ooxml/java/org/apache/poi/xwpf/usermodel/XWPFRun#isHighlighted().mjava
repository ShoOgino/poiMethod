    @Override
    public boolean isHighlighted() {
        CTRPr pr = run.getRPr();
        if (pr == null || !pr.isSetHighlight()) {
            return false;
        }
        if (pr.getHighlight().getVal() == STHighlightColor.NONE) {
            return false;
        }
        return true;
    }
