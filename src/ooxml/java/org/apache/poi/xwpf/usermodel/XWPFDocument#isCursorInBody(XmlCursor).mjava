    /**
     * verifies that cursor is on the right position
     * @param cursor
     */
    private boolean isCursorInBody(XmlCursor cursor) {
	XmlCursor verify = cursor.newCursor();
	verify.toParent();
	try {
	    return (verify.getObject() == this.ctDocument.getBody());
	} finally {
	    verify.dispose();
	}
    }

