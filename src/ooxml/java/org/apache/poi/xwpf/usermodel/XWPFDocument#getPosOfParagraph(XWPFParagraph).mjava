	/**
	 * get position of the paragraph
	 * @param p
	 */
	public Integer getPosOfParagraph(XWPFParagraph p){
    	int i, pos = 0;
    	for (i = 0 ; i < bodyElements.size() ; i++) {
    		if (bodyElements.get(i) instanceof XWPFParagraph){
    			if (bodyElements.get(i).equals(p)){
    				return pos;
    			}
    			pos++;
    		}
		}
    	return null;
    }

