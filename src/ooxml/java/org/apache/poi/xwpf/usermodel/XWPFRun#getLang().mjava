    /**
     * Get the language tag associated with this run, if any.
     *
     * @return the language tag associated with this run, if any
     */
    public String getLang() {
        CTRPr pr = run.getRPr();
        Object lang = pr == null || !pr.isSetLang() ? null : pr.getLang().getVal();
        return (String) lang;
    }

