    /**
     * For isBold, isItalic etc
     */
    private boolean isCTOnOff(CTOnOff onoff) {
       if(! onoff.isSetVal())
          return true;
       if(onoff.getVal() == STOnOff.ON)
          return true;
       if(onoff.getVal() == STOnOff.TRUE)
          return true;
       return false;
    }

