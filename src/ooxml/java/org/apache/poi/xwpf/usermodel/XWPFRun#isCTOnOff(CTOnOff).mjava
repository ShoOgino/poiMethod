    /**
     * For isBold, isItalic etc
     */
    private static boolean isCTOnOff(CTOnOff onoff) {
        if (!onoff.isSetVal())
            return true;
        final STOnOff.Enum val = onoff.getVal();
        return (
            (STOnOff.TRUE == val) ||
            (STOnOff.X_1 == val) ||
            (STOnOff.ON == val)
        );
    }

