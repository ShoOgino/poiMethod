    /**
     * add a footnote to the document
     * @param note
     * @throws IOException		 
     */
    public XWPFFootnote addFootnote(CTFtnEdn note){
        CTFtnEdn newNote = ctFootnotes.addNewFootnote();
        newNote.set(note);
        XWPFFootnote xNote = new XWPFFootnote(newNote, this);
        listFootnote.add(xNote);
        return xNote;
    }

