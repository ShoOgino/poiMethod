    /**
     * verifies that cursor is on the right position
     *
     * @param cursor
     */
    private boolean isCursorInHdrF(XmlCursor cursor) {
        XmlCursor verify = cursor.newCursor();
        verify.toParent();
        if (verify.getObject() == this.headerFooter) {
            return true;
        }
        return false;
    }

