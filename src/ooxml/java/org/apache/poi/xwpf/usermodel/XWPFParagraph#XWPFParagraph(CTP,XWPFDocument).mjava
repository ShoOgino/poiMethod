    public XWPFParagraph(CTP prgrph, XWPFDocument docRef)
    {
        super(prgrph);
        
        this.docRef = docRef; 
        CTR[] rs = paragraph.getRArray();
    
        // Get text
        for (int j = 0; j < rs.length; j++) {
            // Loop over text runs
            CTText[] texts = rs[j].getTArray();
            for (int k = 0; k < texts.length; k++) {
                text.append(
                        texts[k].getStringValue()
                );
            }
        }
    }

