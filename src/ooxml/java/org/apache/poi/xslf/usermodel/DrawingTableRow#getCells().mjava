    public DrawingTableCell[] getCells() {
        List<CTTableCell> ctTableCells = row.getTcList();
        DrawingTableCell[] o = new DrawingTableCell[ctTableCells.size()];

        for (int i=0; i<o.length; i++) {
            o[i] = new DrawingTableCell(ctTableCells.get(i));
        }

        return o;
    }

