    public DrawingTableRow[] getRows() {
        List<CTTableRow> ctTableRows = table.getTrList();
        DrawingTableRow[] o = new DrawingTableRow[ctTableRows.size()];

        for (int i=0; i<o.length; i++) {
            o[i] = new DrawingTableRow(ctTableRows.get(i));
        }

        return o;
    }

