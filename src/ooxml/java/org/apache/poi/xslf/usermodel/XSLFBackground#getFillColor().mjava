    @Override
    public Color getFillColor(){
        FillStyle fs = getFillStyle();
        PaintStyle ps = fs.getPaint();
        if (ps instanceof SolidPaint) {
            SolidPaint sp = (SolidPaint)ps;
            ColorStyle cs = sp.getSolidColor();
            return DrawPaint.applyColorTransform(cs);
        }
        return null;
    }

