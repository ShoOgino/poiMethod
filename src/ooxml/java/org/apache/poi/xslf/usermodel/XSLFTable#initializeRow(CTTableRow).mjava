    private XSLFTableRow initializeRow(CTTableRow tr) {
        XSLFTableRow row = new XSLFTableRow(tr, this);
        // default height is 20 points
        row.setHeight(20.0);
        _rows.add(row);
        for (int i = 0;  i < getNumberOfColumns(); i++) {
            row.addCell();
        }
        return row;
    }

