    protected CTShapeProperties getSpPr(){
        if(_spPr == null) {
            for(XmlObject obj : _shape.selectPath("*")){
                if(obj instanceof CTShapeProperties){
                    _spPr = (CTShapeProperties)obj;
                }
            }
        }
        if(_spPr == null) {
            throw new IllegalStateException("CTShapeProperties was not found.");
        }
        return _spPr;
    }

