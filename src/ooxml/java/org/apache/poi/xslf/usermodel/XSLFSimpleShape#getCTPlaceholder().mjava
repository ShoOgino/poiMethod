    protected CTPlaceholder getCTPlaceholder(){
        if(_ph == null){
            XmlObject[] obj = _shape.selectPath(
                    "declare namespace p='http://schemas.openxmlformats.org/presentationml/2006/main' .//*/p:nvPr/p:ph");
            if(obj.length == 1){
                _ph = (CTPlaceholder)obj[0];
            }
        }
        return _ph;
    }

