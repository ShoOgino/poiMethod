    @Override
    protected java.awt.Shape getOutline() {
        Rectangle2D anchor = getAnchor();
        double x1 = anchor.getX(),
                y1 = anchor.getY(),
                x2 = anchor.getX() + anchor.getWidth(),
                y2 = anchor.getY() + anchor.getHeight();


        return new Line2D.Double(x1, y1, x2, y2);
    }

