    public TextRun getTextRun() {
        for (XSLFShape sh : super.getShapes()) {
            if (sh instanceof XSLFTextShape) {
                XSLFTextShape txt = (XSLFTextShape)sh;
                for (XSLFTextParagraph p : txt.getTextParagraphs()) {
                    for (XSLFTextRun r : p.getTextRuns()) {
                        return r;
                    }
                }
            }
        }
        return null;
    }

