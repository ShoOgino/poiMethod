    public DrawingParagraph[] getParagraphs() {
        CTTextParagraph[] pArray = textBody.getPArray();
        DrawingParagraph[] o = new DrawingParagraph[pArray.length];

        for (int i=0; i<o.length; i++) {
            o[i] = new DrawingParagraph(pArray[i]);
        }

        return o;
    }

