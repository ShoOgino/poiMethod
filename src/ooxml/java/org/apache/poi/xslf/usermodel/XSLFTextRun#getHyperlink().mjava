    @Override
    public XSLFHyperlink getHyperlink(){
        CTTextCharacterProperties rPr = _r.getRPr();
        if (rPr == null) { 
            return null;
        }
        CTHyperlink hl = rPr.getHlinkClick();
        if (hl == null) {
            return null;
        }
        return new XSLFHyperlink(hl, _p.getParentShape().getSheet());
    }

