    private short getMaxOutlineLevelRows(){
        short outlineLevel=0;
        for(Row r : rows.values()){
            XSSFRow xrow=(XSSFRow)r;
            outlineLevel=xrow.getCTRow().getOutlineLevel()>outlineLevel? xrow.getCTRow().getOutlineLevel(): outlineLevel;
        }
        return outlineLevel;
    }

