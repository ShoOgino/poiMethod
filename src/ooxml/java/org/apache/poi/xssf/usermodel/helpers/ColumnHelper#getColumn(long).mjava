    public CTCol getColumn(long index) {
        for (int i = 0; i < worksheet.getColsArray(0).sizeOfColArray(); i++) {
            if (worksheet.getColsArray(0).getColArray(i).getMin() == index) {
                return worksheet.getColsArray(0).getColArray(i);
            }
        }
        return null;
    }

