    public void removeRowBreak(int row) {
        CTBreak[] brkArray = worksheet.getRowBreaks().getBrkArray();
        for (int i = 0 ; i < brkArray.length ; i++) {
            if (brkArray[i].getId() == row) {
                worksheet.getRowBreaks().removeBrk(i);
                continue;
            }
        }
    }

