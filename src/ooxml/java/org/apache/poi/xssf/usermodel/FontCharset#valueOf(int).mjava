    public static FontCharset valueOf(int value){
	switch (value) {
	case 0:
	    return ANSI;
	case 1:
	    return DEFAULT;
	case 2:
	    return SYMBOL;
	case 77:
	    return MAC;
	case 128:
	    return SHIFTJIS;
	case 129:
	    return HANGEUL;
	case 130:
	    return JOHAB;
	case 134:
	    return GB2312;
	case 136:
	    return CHINESEBIG5;
	case 161:
	    return GREEK;
	case 162:
	    return TURKISH;
	case 163:
	    return VIETNAMESE;
	case 177:
	    return HEBREW;
	case 178:
	    return ARABIC;
	case 186:
	    return BALTIC;
	case 204:
	    return RUSSIAN;
	case 222:
	    return THAI;
	case 238:
	    return EASTEUROPE;
	case 255:
	    return OEM;
	}
	throw new RuntimeException("Charset value ["+ value +"] not supported");    
    }

