    /**
     * Returns the plain string representation.
     */
    public String getString() {
        if(st.sizeOfRArray() == 0) {
            return st.getT();
        }
        StringBuffer buf = new StringBuffer();
        for(CTRElt r : st.getRList()){
            buf.append(r.getT());
        }
        return buf.toString();
    }

