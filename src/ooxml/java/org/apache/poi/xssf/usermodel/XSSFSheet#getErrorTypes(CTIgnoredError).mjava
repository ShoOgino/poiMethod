    private Set<IgnoredErrorType> getErrorTypes(CTIgnoredError err) {
        Set<IgnoredErrorType> result = new LinkedHashSet<IgnoredErrorType>();
        for (IgnoredErrorType errType : IgnoredErrorType.values()) {
            if (errType.isSet(err)) {
                result.add(errType);
            }
        }
        return result;
    }

