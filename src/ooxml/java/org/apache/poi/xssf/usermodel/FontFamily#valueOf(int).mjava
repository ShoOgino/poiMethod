    public static FontFamily valueOf(int family){
	switch (family) {
	case 0:
	    return NOT_APPLICABLE;
	case 1:
	    return ROMAN;
	case 2:
	    return SWISS;
	case 3:
	    return MODERN;
	case 4:
	    return SCRIPT;
	case 5:
	    return DECORATIVE;
	}
	throw new RuntimeException("Family value ["+ family +"] not supported");    
    }

