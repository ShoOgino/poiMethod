    public boolean getBooleanCellValue() {
        if (STCellType.B != cell.getT()) { 
            throw new NumberFormatException("You cannot get a boolean value from a non-boolean cell");
        }
        if (cell.isSetV()) {
            return (TRUE_AS_STRING.equals(this.cell.getV()));            
        }
        
        return false;
    }

