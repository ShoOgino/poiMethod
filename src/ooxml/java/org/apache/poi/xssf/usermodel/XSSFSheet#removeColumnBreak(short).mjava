    public void removeColumnBreak(short column) {
        CTBreak[] brkArray = worksheet.getColBreaks().getBrkArray();
        for (int i = 0 ; i < brkArray.length ; i++) {
            if (brkArray[i].getId() == column) {
                worksheet.getColBreaks().removeBrk(i);
                continue;
            }
        }
    }

