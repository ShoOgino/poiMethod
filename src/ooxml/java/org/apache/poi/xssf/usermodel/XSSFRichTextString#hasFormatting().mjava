    /**
     * Does this string have any explicit formatting applied, or is 
     *  it just text in the default style?
     */
    public boolean hasFormatting() {
        CTRElt[] rs = st.getRArray();
        if (rs == null || rs.length == 0) {
            return false;
        }
        for (CTRElt r : rs) {
            if (r.isSetRPr()) return true;
        }
        return false;
    }

