    public XSSFName getSpecificBuiltinRecord(String builtInCode, int sheetNumber) {
        for (XSSFName name : namedRanges) {
            if (name.getNameName().equalsIgnoreCase(builtInCode) && name.getLocalSheetId() == sheetNumber) {
                return name;
            }
        }
        return null;
    }

