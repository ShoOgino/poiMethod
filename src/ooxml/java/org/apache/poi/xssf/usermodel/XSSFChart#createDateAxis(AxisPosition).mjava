	public XSSFDateAxis createDateAxis(AxisPosition pos) {
	    long id = axis.size() + 1;
	    XSSFDateAxis dateAxis = new XSSFDateAxis(this, id, pos);
	    if (axis.size() == 1) {
	        ChartAxis ax = axis.get(0);
	        ax.crossAxis(dateAxis);
	        dateAxis.crossAxis(ax);
	    }
	    axis.add(dateAxis);
	    return dateAxis;
	}

