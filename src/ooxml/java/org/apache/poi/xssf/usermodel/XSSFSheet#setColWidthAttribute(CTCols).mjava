    /**
     * Do not leave the width attribute undefined (see #52186).
     */
    private void setColWidthAttribute(CTCols ctCols) {
        for (CTCol col : ctCols.getColList()) {
        	if (!col.isSetWidth()) {
        		col.setWidth(getDefaultColumnWidth());
        		col.setCustomWidth(false);
        	}
        }
    }

