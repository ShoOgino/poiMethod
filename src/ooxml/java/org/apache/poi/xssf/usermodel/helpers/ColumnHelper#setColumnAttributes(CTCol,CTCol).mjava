    public void setColumnAttributes(CTCol col, CTCol newCol) {
        if (col.getWidth() != 0) {
            newCol.setWidth(col.getWidth());
        }
        if (col.getHidden()) {
            newCol.setHidden(true);
        }
        if (col.getBestFit()) {
            newCol.setBestFit(true);
        }
    }

