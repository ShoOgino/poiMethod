    public void setColumnAttributes(CTCol fromCol, CTCol toCol) {
        if (fromCol.getWidth() != 0) {
            toCol.setWidth(fromCol.getWidth());
        }
        if (fromCol.getHidden()) {
            toCol.setHidden(true);
        }
        if (fromCol.getBestFit()) {
            toCol.setBestFit(true);
        }
    }

