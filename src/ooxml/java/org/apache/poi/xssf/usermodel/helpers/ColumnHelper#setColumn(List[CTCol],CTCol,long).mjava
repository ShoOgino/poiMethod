	private void setColumn(List<CTCol> columns, CTCol col, long k) {
		CTCol column = getColumn(columns, k);
		if (column == null) {
			column = CTCol.Factory.newInstance();
			column.setMin(k);
			column.setMax(k);
			setColumnAttributes(col, column);
			columns.add(column);
		}
		else {
			setColumnAttributes(col, column);
		}
	}

