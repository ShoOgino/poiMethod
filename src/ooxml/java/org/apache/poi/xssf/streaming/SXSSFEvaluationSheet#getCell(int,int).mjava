    public EvaluationCell getCell(int rowIndex, int columnIndex) {
        SXSSFRow row = _xs.getRow(rowIndex);
        if (row == null) {
            return null;
        }
        SXSSFCell cell = row.getCell(columnIndex);
        if (cell == null) {
            return null;
        }
        return new SXSSFEvaluationCell(cell, this);
    }

