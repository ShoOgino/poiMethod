    public void setFontSize(Double size) {
        if (size == null) {
            if (props.isSetSz()) {
                props.unsetSz();
            }
        } else if (size < 1 || 400 < size) {
            throw new IllegalArgumentException("Minimum inclusive = 1. Maximum inclusive = 400.");
        } else {
            props.setSz((int)(100 * size));
        }
    }

