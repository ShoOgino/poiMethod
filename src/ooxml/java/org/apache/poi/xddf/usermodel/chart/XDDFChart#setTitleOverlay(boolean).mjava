    /**
     * @since 4.0.1
     */
    public void setTitleOverlay(boolean overlay) {
        if (!chart.isSetTitle()) {
            chart.addNewTitle();
        }
        CTTitle title = chart.getTitle();
        if (title.isSetOverlay()) {
            title.getOverlay().setVal(overlay);
        } else {
            title.addNewOverlay().setVal(overlay);
        }
    }

