    public List<XDDFGradientStop> getGradientStops() {
        if (props.isSetGsLst()) {
            return Collections.unmodifiableList(props
                .getGsLst()
                .getGsList()
                .stream()
                .map(gs -> new XDDFGradientStop(gs))
                .collect(Collectors.toList()));
        } else {
            return Collections.emptyList();
        }
    }

