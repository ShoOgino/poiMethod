    @Test
    public void testSetGetVanish() throws IOException {
        XWPFDocument document = new XWPFDocument();
        final XWPFRun run = document.createParagraph().createRun();
        assertEquals(false, run.isVanish());
        run.setVanish(true);
        assertEquals(true, run.isVanish());
        run.setVanish(false);
        assertEquals(false, run.isVanish());
        document.close();
    }

