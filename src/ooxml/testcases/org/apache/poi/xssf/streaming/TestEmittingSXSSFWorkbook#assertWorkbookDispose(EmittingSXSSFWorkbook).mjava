    private static void assertWorkbookDispose(EmittingSXSSFWorkbook wb) {
        populateData(wb, 1000, 5);
        
        for (Sheet sheet : wb) {
            EmittingSXSSFSheet sxSheet = (EmittingSXSSFSheet) sheet;
            assertNull(sxSheet.getSheetDataWriter());
        }
        
        assertTrue(wb.dispose());
        
        for (Sheet sheet : wb) {
            EmittingSXSSFSheet sxSheet = (EmittingSXSSFSheet) sheet;
            assertNull(sxSheet.getSheetDataWriter());
        }
    }

