    /**
     * Returns true if the file should be checked, false if it should be excluded.
     *
     * @param f
     * @return
     */
    public static boolean checkExclude(File f) {
        for(String exclude : excludes) {
            if(f.getAbsolutePath().endsWith(exclude)) {
                return false;
            }
        }
        
        return true;
    }

