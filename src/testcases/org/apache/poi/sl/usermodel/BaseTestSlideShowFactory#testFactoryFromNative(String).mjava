    protected static void testFactoryFromNative(String file) throws Exception {
        SlideShow<?,?> ss;
        // from NPOIFS
        if (!file.contains("pptx")) {
            NPOIFSFileSystem npoifs = new NPOIFSFileSystem(fromFile(file));
            ss = SlideShowFactory.create(npoifs);
            assertNotNull(ss);
            npoifs.close();
            ss.close();
        }
    }

