    public void testWriteProtection() {
        HSSFWorkbook wb=new HSSFWorkbook();
        
        assertFalse(wb.isWriteProtected());
        
        wb.writeProtectWorkbook("mypassword", "myuser");
        assertTrue(wb.isWriteProtected());
        
        wb.unwriteProtectWorkbook();
        assertFalse(wb.isWriteProtected());
    }

