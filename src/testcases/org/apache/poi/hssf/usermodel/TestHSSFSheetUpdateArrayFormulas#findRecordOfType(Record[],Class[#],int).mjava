    /**
     * @return <tt>-1<tt> if not found
     */
    private static int findRecordOfType(Record[] recs, Class<?> type, int fromIndex) {
        for (int i=fromIndex; i<recs.length; i++) {
            if (type.isInstance(recs[i])) {
                return i;
            }
        }
        return -1;
    }

