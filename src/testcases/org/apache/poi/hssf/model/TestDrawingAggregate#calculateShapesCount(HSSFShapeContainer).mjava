    private void calculateShapesCount(HSSFShapeContainer group) {
        for (HSSFShape shape : (List<HSSFShape>) group.getChildren()) {
            if (shape instanceof HSSFShapeGroup) {
                shGroupCount++;
                calculateShapesCount((HSSFShapeGroup) shape);
            } else {
                shapeCount++;
            }
        }
    }

