    @Test
    public void testLoadInvalid() {
        try {
            assertNotNull(new CellRangeAddress(
                TestcaseRecordInputStream.create(0x000, new byte[] { (byte)0x02 })));
        } catch (RuntimeException e) {
            assertTrue("Had: " + e, e.getMessage().contains("Ran out of data"));
        }
    }

