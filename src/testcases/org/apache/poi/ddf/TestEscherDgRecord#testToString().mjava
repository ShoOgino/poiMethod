    @Test
    public void testToString() {
        String nl = System.getProperty("line.separator");
        String expected =
            "org.apache.poi.ddf.EscherDgRecord (Dg):" + nl +
            "  RecordId: 0xF008" + nl +
            "  Version: 0x0000" + nl +
            "  Instance: 0x0001" + nl +
            "  Options: 0x0010" + nl +
            "  Record Size: 16" + nl +
            "  NumShapes: 0x00000002" + nl +
            "  LastMSOSPID: 0x00000401";
        assertEquals( expected, createRecord().toString() );
    }

