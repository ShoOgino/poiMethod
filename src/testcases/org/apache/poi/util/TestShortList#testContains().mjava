    public void testContains() {
        ShortList list = new ShortList();

        for (short j = 0; j < 1000; j += 2)
        {
            list.add(j);
        }
        for (short j = 0; j < 1000; j++)
        {
            if (j % 2 == 0)
            {
                assertTrue(list.contains(j));
            }
            else
            {
                assertTrue(!list.contains(j));
            }
        }
    }

