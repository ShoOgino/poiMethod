    public void testIndexOf() {
        ShortList list = new ShortList();

        for (short j = 0; j < 1000; j++)
        {
            list.add(( short ) (j / 2));
        }
        for (short j = 0; j < 1000; j++)
        {
            if (j < 500)
            {
                assertEquals(j * 2, list.indexOf(j));
            }
            else
            {
                assertEquals(-1, list.indexOf(j));
            }
        }
    }

