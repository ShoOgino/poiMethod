      public int read(byte[] b, int offset, int len) throws IOException {
          if (size >= maxSize) return -1;
          int sz = (int)Math.min(len, maxSize-size);
          for (int i=0; i<sz; i++) {
              b[i+offset] = (byte)((size+i) % 128);
          }
          size += sz;
          return sz;
      }

