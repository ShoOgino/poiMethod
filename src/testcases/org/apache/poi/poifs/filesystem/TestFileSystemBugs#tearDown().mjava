    protected void tearDown() throws Exception {
        if (openedFSs != null && !openedFSs.isEmpty()) {
            for (NPOIFSFileSystem fs : openedFSs) {
                try {
                    fs.close();
                } catch (Exception e) {
                    System.err.println("Error closing FS: " + e);
                }
            }
        }
        openedFSs = null;
    }

