	public void testDetectAsPOIFS() throws IOException {
		// ooxml file isn't
		confirmIsPOIFS("SampleSS.xlsx", false);
		
        // 2003 xml file isn't
        confirmIsPOIFS("SampleSS.xml", false);
        
		// xls file is
		confirmIsPOIFS("SampleSS.xls", true);
		
		// text file isn't
		confirmIsPOIFS("SampleSS.txt", false);
	}

