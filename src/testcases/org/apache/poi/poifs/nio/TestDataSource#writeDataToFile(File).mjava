    private void writeDataToFile(File temp) throws FileNotFoundException, IOException {
        OutputStream str = new FileOutputStream(temp);
           try {
               InputStream in = data.openResourceAsStream("Notes.ole2");
               try {
                   IOUtils.copy(in, str);
               } finally {
                   in.close();
               }
           } finally {
               str.close();
           }
    }

