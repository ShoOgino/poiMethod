    /**
     * <p>Renders {@link DocumentDescriptor} as a string.</p>
     */
    protected String renderAsString(final DocumentDescriptor d) {
        return "Name: " +
                d.name +
                " " +
                HexDump.toHex(d.name) +
                "\n" +
                "Size: " +
                d.size +
                " bytes\n" +
                "First bytes: " +
                HexDump.toHex(d.bytes);
    }

