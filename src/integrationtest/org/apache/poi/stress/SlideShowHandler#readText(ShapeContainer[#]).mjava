    @SuppressWarnings("unchecked")
    private void readText(ShapeContainer<?> sc) {
        if (sc == null) return;
        for (Shape s : sc) {
            if (s instanceof TextShape) {
                for (TextParagraph<? extends TextRun> tp : (TextShape<TextParagraph<? extends TextRun>>)s) {
                    for (TextRun tr : tp) {
                        tr.getRawText();
                    }
                }
            }
        }
    }

