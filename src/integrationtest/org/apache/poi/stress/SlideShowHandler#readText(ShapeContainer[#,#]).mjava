    private void readText(ShapeContainer<?,?> sc) {
        if (sc == null) return;
        for (Shape<?,?> s : sc) {
            if (s instanceof TextShape) {
                for (TextParagraph<?,?,?> tp : (TextShape<?,?>)s) {
                    for (TextRun tr : tp) {
                        tr.getRawText();
                    }
                }
            }
        }
    }

