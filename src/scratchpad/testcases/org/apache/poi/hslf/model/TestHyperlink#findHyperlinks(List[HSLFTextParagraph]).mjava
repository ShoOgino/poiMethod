    private static List<HSLFHyperlink> findHyperlinks(List<HSLFTextParagraph> paras) {
        List<HSLFHyperlink> links = new ArrayList<HSLFHyperlink>();
        for (HSLFTextParagraph p : paras) {
            for (HSLFTextRun r : p) {
                HSLFHyperlink hl = r.getHyperlink();
                if (hl != null) {
                    links.add(hl);
                }
            }
        }
        return links;
    }

