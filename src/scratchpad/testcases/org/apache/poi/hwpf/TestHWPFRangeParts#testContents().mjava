	public void testContents() throws Exception {
		Range r;
		
		// Now check the real ranges
		r = docAscii.getRange();
		assertEquals(
				a_page_1 +
				page_break + "\r" +
				a_page_2,
				r.text()
		);
		
		r = docAscii.getHeaderStoryRange();
		assertEquals(
				headerDef +
				a_header +
				footerDef +
				a_footer + 
				endHeaderFooter,
				r.text()
		);
		
		r = docAscii.getOverallRange();
		assertEquals(
				a_page_1 +
				page_break + "\r" +
				a_page_2 + 
				headerDef +
				a_header +
				footerDef +
				a_footer + 
				endHeaderFooter +
				"\r",
				r.text()
		);
	}

