  public boolean equals(Object o)
  {
    byte[] buf = getBuf();
    if (((PropertyNode)o).getStart() == _cpStart &&
        ((PropertyNode)o).getEnd() == _cpEnd)
    {
      byte[] testBuf = ((PropertyNode)o).getBuf();

      if (testBuf.length == buf.length)
      {
        for (int x = 0; x < buf.length; x++)
        {
          if (testBuf[x] != buf[x])
          {
            return false;
          }
        }
        return true;
      }
    }
    return false;
  }

