    // Ryans code
    private int FCtoCP(int fc)
   {
     int size = _text.size();
     int cp = 0;
     for (int x = 0; x < size; x++)
     {
       TextPiece piece = (TextPiece)_text.get(x);

       if (fc <= piece.getEnd())
       {
         cp += (fc - piece.getStart());
         break;
       }
       else
       {
         cp += (piece.getEnd() - piece.getStart());
       }
     }
     return cp;
   }

