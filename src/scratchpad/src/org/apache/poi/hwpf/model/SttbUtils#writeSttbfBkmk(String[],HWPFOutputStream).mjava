    static void writeSttbfBkmk( String[] data, HWPFOutputStream tableStream )
            throws IOException
    {
        Sttb sttb = new Sttb();
        sttb.cDataLength = CDATA_SIZE_STTBF_BKMK;
        sttb.data = data;
        sttb.cbExtra = CBEXTRA_STTBF_BKMK;
        write( sttb, tableStream );
    }

