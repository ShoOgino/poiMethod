    static void writeSttbSavedBy( String[] data, HWPFOutputStream tableStream )
            throws IOException
    {
        Sttb sttb = new Sttb();
        sttb.cDataLength = CDATA_SIZE_STTB_SAVED_BY;
        sttb.data = data;
        sttb.cbExtra = CBEXTRA_STTB_SAVED_BY;
        write( sttb, tableStream );
    }

