    static void writeSttbfRMark( String[] data, HWPFOutputStream tableStream )
            throws IOException
    {
        Sttb sttb = new Sttb();
        sttb.cDataLength = CDATA_SIZE_STTBF_R_MARK;
        sttb.data = data;
        sttb.cbExtra = CBEXTRA_STTBF_R_MARK;
        write( sttb, tableStream );
    }

