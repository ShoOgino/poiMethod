    public int getSizeInBytes()
    {
        int result = 6 // int byte byte
                + RGBXCH_NUMS_SIZE + 13 // byte int int byte byte short
                + _cbGrpprlChpx + _cbGrpprlPapx + 2; // numberText length
        if ( _numberText != null )
        {
            result += _numberText.length * LittleEndian.SHORT_SIZE;
        }
        return result;
