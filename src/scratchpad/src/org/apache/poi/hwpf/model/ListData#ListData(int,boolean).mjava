  public ListData(int listID, boolean numbered)
  {
    _lsid = listID;
    _rgistd = new short[9];

    for (int x = 0; x < 9; x++)
    {
      _rgistd[x] = StyleSheet.NIL_STYLE;
    }

    _levels = new ListLevel[9];

    for (int x = 0; x < _levels.length; x++)
    {
      _levels[x] = new ListLevel(x, numbered);
    }
  }

