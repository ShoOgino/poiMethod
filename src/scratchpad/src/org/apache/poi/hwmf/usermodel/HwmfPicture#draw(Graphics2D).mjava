    public void draw(Graphics2D ctx) {
        AffineTransform at = ctx.getTransform();
        try {
            HwmfGraphics g = new HwmfGraphics(ctx, getBounds());
            for (HwmfRecord r : records)  {
                r.draw(g);
            }
        } finally {
            ctx.setTransform(at);
        }
    }

