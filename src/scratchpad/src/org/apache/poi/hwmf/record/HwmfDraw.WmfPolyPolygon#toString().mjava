        @Override
        public String toString() {
            final StringBuilder sb = new StringBuilder();
            sb.append("{ polyList: [");
            boolean isFirst = true;
            for (Path2D p : polyList) {
                if (!isFirst) {
                    sb.append(",");
                }
                isFirst = false;
                sb.append("{ points: ");
                sb.append(polyToString(p));
                sb.append(" }");
            }
            sb.append(" }");
            return sb.toString();
        }

