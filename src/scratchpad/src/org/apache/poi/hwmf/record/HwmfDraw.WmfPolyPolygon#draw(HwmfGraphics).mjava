        @Override
        public void draw(HwmfGraphics ctx) {
            int windingRule = ctx.getProperties().getPolyfillMode().awtFlag;
            Area area = new Area();
            for (Path2D poly : polyList) {
                Path2D p = (Path2D)poly.clone();
                p.setWindingRule(windingRule);
                area.add(new Area(p));
            }
            ctx.draw(area);
        }

