        @Override
        public void draw(HwmfGraphics ctx) {
            Shape s = getShape();
            switch (getFillDrawStyle()) {
                case FILL:
                    ctx.fill(s);
                    break;
                case DRAW:
                    ctx.draw(s);
                    break;
                case FILL_DRAW:
                    ctx.fill(s);
                    ctx.draw(s);
                    break;
            }
        }

