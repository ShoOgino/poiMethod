        @Override
        public String toString() {
            Arc2D arc = getShape();
            return
                "{ startPoint: "+pointToString(startPoint)+
                ", endPoint: "+pointToString(endPoint)+
                ", startAngle: "+arc.getAngleStart()+
                ", extentAngle: "+arc.getAngleExtent()+
                ", bounds: "+boundsToString(bounds)+
                " }";
        }

