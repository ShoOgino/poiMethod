    @Override
    public String toString() {
        return "{ height: "+height+
                ", width: "+width+
                ", escapment: "+escapement+
                ", weight: "+weight+
                ", italic: "+italic+
                ", underline: "+underline+
                ", strikeOut: "+strikeOut+
                ", charset: '"+charSet+"'"+
                ", outPrecision: '"+outPrecision+"'"+
                ", clipPrecision: '"+clipPrecision+"'"+
                ", quality: '"+quality+"'"+
                ", pitch: '"+getPitch()+"'"+
                ", family: '"+getFamily()+"'"+
                ", facename: '"+facename+"'"+
                "}";
    }

