    private void textRunsToText(StringBuffer ret, HSLFTextParagraph[] runs) {
        if (runs==null) {
            return;
        }

        for (int j = 0; j < runs.length; j++) {
            HSLFTextParagraph run = runs[j];
            if (run != null) {
                String text = run.getText();
                ret.append(text);
                if (!text.endsWith("\n")) {
                    ret.append("\n");
                }
            }
        }
    }

