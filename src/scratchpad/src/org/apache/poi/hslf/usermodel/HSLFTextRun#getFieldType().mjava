    @Override
    public FieldType getFieldType() {
        Placeholder ph = getTextParagraph().getParentShape().getPlaceholder();
        if (ph != null) {
            switch (ph) {
            case SLIDE_NUMBER:
                return FieldType.SLIDE_NUMBER;
            case DATETIME:
                return FieldType.DATE_TIME;
            default:
                break;
            }
        }
        return null;
    }

