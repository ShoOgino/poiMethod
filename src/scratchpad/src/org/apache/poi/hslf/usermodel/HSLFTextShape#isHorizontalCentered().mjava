    @Override
    public boolean isHorizontalCentered() {
        int va = getAlignment();
        switch (va) {
        case AnchorTopCentered:
        case AnchorTopCenteredBaseline:
        case AnchorBottomCentered:
        case AnchorBottomCenteredBaseline:
        case AnchorMiddleCentered:
            return true;
        default:
            return false;
        }
    }

