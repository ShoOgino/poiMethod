    /**
     * Sets the text contained in this text frame.
     *
     * @param text the text string used by this object.
     */
    public void setText(String text){
        HSLFTextParagraph tx = getTextParagraph();
        if(tx == null){
            tx = createTextRun();
        }
        tx.setText(text);
        setTextId(text.hashCode());
    }

