	public TextProp removeByName(String name) {
	    Iterator<TextProp> iter = textPropList.iterator();
	    TextProp tp = null;
	    while (iter.hasNext()) {
	        tp = iter.next();
	        if (tp.getName().equals(name)){
	            iter.remove();
	            break;
	        }
	    }
	    return tp;
	}

