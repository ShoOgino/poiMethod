    public PPTXMLDump(File ppt) throws IOException {
        POIFSFileSystem fs = new POIFSFileSystem(ppt, true);
        try {
            docstream = readEntry(fs, HSLFSlideShow.POWERPOINT_DOCUMENT);
            pictstream = readEntry(fs, PICTURES_ENTRY);
        } finally {
            fs.close();
        }
    }

