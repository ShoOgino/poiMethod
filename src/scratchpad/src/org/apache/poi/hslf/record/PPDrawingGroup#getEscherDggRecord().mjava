    public EscherDggRecord getEscherDggRecord(){
        if(dgg == null){
            for(Iterator it = dggContainer.getChildRecords().iterator(); it.hasNext();){
                EscherRecord r = (EscherRecord) it.next();
                if(r instanceof EscherDggRecord){
                    dgg = (EscherDggRecord)r;
                    break;
                }
            }
        }
        return dgg;
    }

