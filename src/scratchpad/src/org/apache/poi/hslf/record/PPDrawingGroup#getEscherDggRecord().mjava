    public EscherDggRecord getEscherDggRecord(){
        if(dgg == null){
            for(Iterator<EscherRecord> it = dggContainer.getChildIterator(); it.hasNext();){
                EscherRecord r = it.next();
                if(r instanceof EscherDggRecord){
                    dgg = (EscherDggRecord)r;
                    break;
                }
            }
        }
        return dgg;
    }

