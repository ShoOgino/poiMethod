		/**
		 * Set the true/false status of the subproperty with the given index
		 */
		public void setSubValue(boolean value, int idx) {
			if(subPropMatches[idx] == value) { return; }
			if(value) {
				dataValue += subPropMasks[idx];
			} else {
				dataValue -= subPropMasks[idx];
			}
			subPropMatches[idx] = value;
		}

