    private static void polyTo(final HemfGraphics ctx, final Path2D poly) {
        final PathIterator pi = poly.getPathIterator(null);
        if (pi.isDone()) {
            // ignore empty polys
            return;
        }

        // ignore dummy start point (moveTo)
        pi.next();
        assert (!pi.isDone());

        ctx.draw((path) -> path.append(pi, true));
    }

