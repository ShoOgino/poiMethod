    @Override
    public HemfPlusRecord next() {
        HemfPlusRecord toReturn = currentRecord;
        final boolean isEOF = (limit == -1 || leis.getReadIndex()-startIdx >= limit);
        // (currentRecord instanceof HemfPlusMisc.EmfEof)
        currentRecord = isEOF ? null : _next();
        return toReturn;
    }

