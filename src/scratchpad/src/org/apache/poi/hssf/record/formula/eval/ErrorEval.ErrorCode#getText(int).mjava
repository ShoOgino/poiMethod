        public static final String getText(int errorCode) {
            switch(errorCode) {
                case NULL:  return "#NULL!";
                case DIV_0: return "#DIV/0!";
                case VALUE: return "#VALUE!";
                case REF:   return "#REF!";
                case NAME:  return "#NAME?";
                case NUM:   return "#NUM!";
                case N_A:   return "#N/A";
            }
            return "???";
        }

