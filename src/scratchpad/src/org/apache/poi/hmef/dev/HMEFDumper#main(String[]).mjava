   public static void main(String[] args) throws Exception {
      if(args.length < 1) {
         throw new IllegalArgumentException("Filename must be given");
      }
      
      for(String filename : args) {
         HMEFDumper dumper = new HMEFDumper(
               new FileInputStream(filename)
         );
         dumper.dump();
      }
   }

