    private static boolean findMagic(byte[] cmp, byte[] actual) {
        int i=0;
        for (byte m : cmp) {
            byte d = actual[i++];
            if (!(d == m || (m == 0x70 && (d == 0x10 || d == 0x20 || d == 0x40)))) {
                return false;
            }
        }
        return true;
    }

