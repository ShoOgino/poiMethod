    protected boolean isValidIndex(int index) {
        if (! Property.isValidIndex(index))
            return false;
        if (index < 0 || index >= _properties.size()) {
            _logger.log(POILogger.WARN, "Property index " + index +
                        "outside the valid range 0.."+_properties.size());
            return false;
        }
        return true;
    }

