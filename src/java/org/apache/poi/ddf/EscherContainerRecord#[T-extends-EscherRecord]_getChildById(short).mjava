    public <T extends EscherRecord> T getChildById( short recordId )
    {
        for ( EscherRecord childRecord : _childRecords )
        {
            if ( childRecord.getRecordId() == recordId )
            {
                @SuppressWarnings( "unchecked" )
                final T result = (T) childRecord;
                return result;
            }
        }
        return null;
    }

