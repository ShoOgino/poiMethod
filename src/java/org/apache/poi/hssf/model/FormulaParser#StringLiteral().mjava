    private void StringLiteral() {
        Match('"');
		  StringBuffer Token = new StringBuffer();
	 	  for(;;) {
		  	     if(look == '"') {
		        GetChar();
		        SkipWhite(); //potential white space here since it doesnt matter up to the operator
		        if(look == '"')
		            Token.append("\"");
		        else
		            break;
		    } else if(look == 0) {
		        break;
		    } else {
		        Token.append(look);
		        GetChar();
		    }
		}
		tokens.add(new StringPtg(Token.toString()));        
    }

