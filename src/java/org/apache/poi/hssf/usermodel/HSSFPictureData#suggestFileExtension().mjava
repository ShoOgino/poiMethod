    /* (non-Javadoc)
     * @see org.apache.poi.hssf.usermodel.PictureData#suggestFileExtension()
     */
    public String suggestFileExtension()
    {
        switch (blip.getOptions() & FORMAT_MASK)
        {
            case MSOBI_WMF:
                return "wmf";
            case MSOBI_EMF:
                return "emf";
            case MSOBI_PICT:
                return "pict";
            case MSOBI_PNG:
                return "png";
            case MSOBI_JPEG:
                return "jpeg";
            case MSOBI_DIB:
                return "dib";
            default:
                return "";
        }
    }

