	public void setFormula2(String formula)
	{
		// Formula 2
		if( formula != null)
		{
		    Stack parsedExpression = parseFormula(formula);
			if( parsedExpression != null )
			{
				cfRuleRecord.setParsedExpression2(parsedExpression);
			}
			else
			{
				cfRuleRecord.setParsedExpression2(null);
			}
		}
		else
		{
			cfRuleRecord.setParsedExpression2(null);
		}
	}

