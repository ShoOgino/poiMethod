	public void setFormula1(String formula)
	{
		// Formula 1
		if( formula != null)
		{
		    Stack parsedExpression = parseFormula(formula);
			if( parsedExpression != null )
			{
				cfRuleRecord.setParsedExpression1(parsedExpression);
			}
			else
			{
				cfRuleRecord.setParsedExpression1(null);
			}
		}
		else
		{
			cfRuleRecord.setParsedExpression1(null);
		}
	}

