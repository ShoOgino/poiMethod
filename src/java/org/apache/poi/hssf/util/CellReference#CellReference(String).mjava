    public CellReference(String cellRef) {
        String[] parts = seperateRowColumns(cellRef);
        String ref = parts[0];
        if (ref.charAt(0) == '$') {
            colAbs=true; 
            ref=ref.substring(1);
        }
        col = convertColStringToNum(ref);
        ref=parts[1];
        if (ref.charAt(0) == '$') {
            rowAbs=true; 
            ref=ref.substring(1);
        }
        row = Integer.parseInt(ref)-1;
    }

