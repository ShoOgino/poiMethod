public String toString() {        StringBuffer buffer = new StringBuffer();                buffer.append("[EXTERNSHEET]\n");        buffer.append("   numOfRefs     = ").append(getNumOfREFStructures()).append("\n");        for (int k=0; k < this.getNumOfREFRecords(); k++) {            buffer.append("refrec         #").append(k).append('\n');            buffer.append(getREFRecordAt(k).toString());            buffer.append("----refrec     #").append(k).append('\n');        }        buffer.append("[/EXTERNSHEET]\n");                        return buffer.toString();    }
