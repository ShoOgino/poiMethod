    private short getTotalPtgSize(List list)
    {
    	short  retval = 0;
    	if( list!=null)
    	{
            for (int k = 0; k < list.size(); k++)
            {
                Ptg ptg = ( Ptg ) list.get(k);

                retval += ptg.getSize();
            }
    	}
        return retval;
    }

