    /**
     * Initialize a new document link
     */
    public void newDocumentLink(){
        rwFirst = 0;
        rwLast = 0;
        colFirst = 0;
        colLast = 0;
        guid = STD_MONIKER;
        label_opts = 0x2;
        link_opts = HLINK_LABEL | HLINK_PLACE;
        label = "" + '\u0000';
        moniker = FILE_MONIKER;
        address = "" + '\0';
        tail = new byte[]{};
    }

