    /**
     * Initialize a new url link
     */
    public void newUrlLink(){
        rwFirst = 0;
        rwLast = 0;
        colFirst = 0;
        colLast = 0;
        guid = STD_MONIKER;
        label_opts = 0x2;
        link_opts = HLINK_URL | HLINK_ABS | HLINK_LABEL;
        label = "" + '\u0000';
        moniker = URL_MONIKER;
        address = "" + '\u0000';
        tail = URL_TAIL;
    }

