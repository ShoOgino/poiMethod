    public String toString()
    {
        StringBuffer buffer = new StringBuffer();

        buffer.append("[AxisUsed]\n");

        buffer.append("    .numAxis              = ")
            .append("0x")
            .append(HexDump.toHex((short)getNumAxis()))
            .append(" (").append(getNumAxis()).append(" )\n");

        buffer.append("[/AxisUsed]\n");
        return buffer.toString();
    }

