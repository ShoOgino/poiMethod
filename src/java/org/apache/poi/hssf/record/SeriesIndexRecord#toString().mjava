    public String toString()
    {
        StringBuffer buffer = new StringBuffer();

        buffer.append("[SeriesIndex]\n");

        buffer.append("    .index                = ")
            .append("0x")
            .append(HexDump.toHex((short)getIndex()))
            .append(" (").append(getIndex()).append(" )\n");

        buffer.append("[/SeriesIndex]\n");
        return buffer.toString();
    }

