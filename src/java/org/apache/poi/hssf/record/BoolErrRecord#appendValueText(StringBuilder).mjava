	@Override
	protected void appendValueText(StringBuilder sb) {
		if (isBoolean()) {
			sb.append("  .boolVal = ");
			sb.append(getBooleanValue());
		} else {
			sb.append("  .errCode = ");
			sb.append(ErrorConstants.getText(getErrorValue()));
			sb.append(" (").append(HexDump.byteToHex(getErrorValue())).append(")");
		}
	}

