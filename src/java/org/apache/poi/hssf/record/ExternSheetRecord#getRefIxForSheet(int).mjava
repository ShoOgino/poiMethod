	public int getRefIxForSheet(int sheetIndex) {
        int nItems = _list.size();
        for (int i = 0; i < nItems; i++) {
            RefSubRecord ref = getRef(i);
			if (ref.getFirstSheetIndex() == sheetIndex && ref.getLastSheetIndex() == sheetIndex) {
            	return i;
            }
        }
		return -1;
	}

