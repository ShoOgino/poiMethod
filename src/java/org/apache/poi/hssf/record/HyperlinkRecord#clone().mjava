    public Object clone() {
        HyperlinkRecord rec = new HyperlinkRecord();
        rec.rwFirst = rwFirst;
        rec.rwLast = rwLast;
        rec.colFirst = colFirst;
        rec.colLast = colLast;
        rec.guid = guid;
        rec.label_opts = label_opts;
        rec.link_opts = link_opts;
        rec.file_opts = file_opts;
        rec.label = label;
        rec.address = address;
        rec.moniker = moniker;
        rec.tail = tail;
        return rec;
    }

