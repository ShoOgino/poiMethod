    private Stack getParsedExpressionTokens(short size,  RecordInputStream in )
    {
        Stack stack = new Stack();
        int pos = 0;
        while ( pos < size )
        {
            Ptg ptg = Ptg.createPtg( in );
            pos += ptg.getSize();
            stack.push( ptg );
        }
        return stack;
    }

