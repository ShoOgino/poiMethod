	public String toString() {
		StringBuffer sb = new StringBuffer();

		sb.append("[OBJ]\n");
		for (int i = 0; i < subrecords.size(); i++) {
			SubRecord record = (SubRecord) subrecords.get(i);
			sb.append("SUBRECORD: ").append(record.toString());
		}
		sb.append("[/OBJ]\n");
		return sb.toString();
	}

