	@Override
	protected void serialize(LittleEndianOutput out) {
		
		out.writeShort(sxaxis);
		out.writeShort(cSub);
		out.writeShort(grbitSub);
		out.writeShort(cItm);
		
		if (name != null) {
			StringUtil.writeUnicodeString(out, name);
		} else {
			out.writeShort(STRING_NOT_PRESENT_LEN);
		}
	}

