	private static int countAreaEval(AreaEval ae) {
		
		int temp = 0;
		ValueEval[] values = ae.getValues();
		for (int i = 0; i < values.length; i++) {
			ValueEval val = values[i];
			if(val == null) {
				// seems to occur.  Really we would have expected BlankEval
				continue;
			}
			temp += countValue(val);
			
		}
		return temp;
	}

