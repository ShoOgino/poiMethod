    private void Expression() {
        if (IsAddop(Look)) {
            EmitLn("CLR D0");  //unaryAdd ptg???
        } else {
            Term();
        }
        while (IsAddop(Look)) {
            EmitLn("MOVE D0,-(SP)");
            if ( Look == '+' )  Add();
            if (Look == '-') Subtract();
        }
    }

