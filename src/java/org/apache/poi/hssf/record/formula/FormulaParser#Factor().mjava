   /** Parse and Translate a Math Factor  */
    private void Factor() {
        if (Look == '(' ) {
            Match('(');
            Expression();
            Match(')');
            tokens.add(new ParenthesisPtg());
            return;
        } else if (IsAlpha(Look)){
            Ident();
        }else{

            IntPtg p = new IntPtg();
            p.setValue(Short.parseShort(GetNum()));
            tokens.add(p);
        }
    }

