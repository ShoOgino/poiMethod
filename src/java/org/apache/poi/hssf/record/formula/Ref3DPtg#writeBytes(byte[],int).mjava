    public void writeBytes(byte [] array, int offset) {
        array[ 0 + offset ] = sid;
        LittleEndian.putShort(array, 1 + offset , getExternSheetIndex());
        LittleEndian.putShort(array, 3 + offset , getRow());
        LittleEndian.putShort(array, 5 + offset , getColumnRaw());               
    }

