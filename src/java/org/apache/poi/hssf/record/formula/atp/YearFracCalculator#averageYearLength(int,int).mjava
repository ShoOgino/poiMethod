	private static double averageYearLength(int startYear, int endYear) {
		int dayCount = 0;
		for (int i=startYear; i<=endYear; i++) {
			dayCount += DAYS_PER_NORMAL_YEAR;
			if (isLeapYear(i)) {
				dayCount++;
			}
		}
		double numberOfYears = endYear-startYear+1;
		return dayCount / numberOfYears;
	}

