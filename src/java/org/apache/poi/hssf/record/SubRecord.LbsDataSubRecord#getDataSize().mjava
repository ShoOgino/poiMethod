		protected int getDataSize() {
			int result = 2; // 2 initial shorts

			// optional link formula
			if (_linkPtg != null) {
				result += 2; // encoded Ptg size
				result += 4; // unknown int
				result += _linkPtg.getSize();
				if (_unknownByte6 != null) {
					result += 1;
				}
			}
			result += 7 * 2; // 7 shorts
			return result;
		}

