       public int compareTo(Object obj) {
           CellValueRecordInterface cell = (CellValueRecordInterface) obj;
  
           if (getRow() == cell.getRow() && cell.getColumn() == 0) {
             return 0;
           }
           else if (getRow() < cell.getRow()) {
             return -1;
           }
           else if (getRow() > cell.getRow()){
             return 1;
           }
           if (cell.getColumn() > 0)
           {
               return -1;
           }
           if (cell.getColumn() < 0)
           {
               return 1;
           }
           return -1;         
       }

