	/**
	 * Performs a deep clone of the record
	 */
	public Object clone() {
		ColumnInfoRecordsAggregate rec = new ColumnInfoRecordsAggregate();
		for (int k = 0; k < records.size(); k++) {
			ColumnInfoRecord ci = ( ColumnInfoRecord ) records.get(k);
			rec.records.add(ci.clone());
		}
		return rec;
	}

