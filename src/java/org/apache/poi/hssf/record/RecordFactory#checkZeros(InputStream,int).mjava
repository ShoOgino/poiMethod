	private static void checkZeros(InputStream in, int avail) throws IOException {
		int count=0;
		while(true) {
			int b = in.read();
			if (b < 0) {
				break;
			}
			if (b!=0) {
				System.err.print(HexDump.byteToHex(b));
			}
			count++;
		}
		if (avail != count) {
			System.err.println("avail!=count (" + avail + "!=" + count + ").");
		}
	}

