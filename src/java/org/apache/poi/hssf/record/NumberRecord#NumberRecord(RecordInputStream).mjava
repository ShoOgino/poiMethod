    /**
     * @param in the RecordInputstream to read the record from
     */
    public NumberRecord(RecordInputStream in)
    {
        field_1_row   = in.readUShort();
        field_2_col   = in.readUShort();
        field_3_xf    = in.readUShort();
        field_4_value = in.readDouble();
    }

