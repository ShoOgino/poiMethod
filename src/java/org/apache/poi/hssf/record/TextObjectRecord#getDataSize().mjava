	/**
	 * Only for the current record. does not include any subsequent Continue
	 * records
	 */
	protected int getDataSize() {
		int result = 2 + 2 + 2 + 2 + 2 + 2 + 2 + 4;
		if (_linkRefPtg != null) {
			result += 2 // formula size
				+ 4  // unknownInt
				+_linkRefPtg.getSize();
			if (_unknownPostFormulaByte != null) {
				result += 1;
			}
		}
		return result;
	}

