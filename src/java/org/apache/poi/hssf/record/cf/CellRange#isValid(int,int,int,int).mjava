	private static boolean isValid(int firstRow, int lastRow, int firstColumn, int lastColumn)
	{
		if(lastRow == -1) {
			if(firstRow !=0) {
				return false;
			}
		}
		if(firstRow < 0 || lastRow < -1) {
			return false;
		}
		
		if(lastColumn == -1) {
			if(firstColumn !=0) {
				return false;
			}
		}
		if(firstColumn < 0 || lastColumn < -1) {
			return false;
		}
		return true;
	}

