    public void serialize(LittleEndianOutput out) {
        out.writeByte(type);
        if (formula == null) {
            out.writeShort(0);
        } else {
            formula.serialize(out);
        }
        if (value != null) {
            out.writeDouble(value);
        }
        out.writeByte(equals);
        out.writeInt(0); // Reserved
    }

