    private static boolean isRowOrCol(String str) {
        int i=0;
        if (str.charAt(i) == '$') {
            i++;
        }
        if (IsDigit(str.charAt(i))) {
            while (i<str.length()) {
                if (!IsDigit(str.charAt(i))) {
                    return false;
                }
                i++;
            }
            return true;
        }
        if (IsAlpha(str.charAt(i))) {
            while (i<str.length()) {
                if (!IsAlpha(str.charAt(i))) {
                    return false;
                }
                i++;
            }
            return true;
        }
        
        return false;
    }

