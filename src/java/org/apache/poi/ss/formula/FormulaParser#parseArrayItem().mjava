	private Object parseArrayItem() {
		SkipWhite();
		switch(look) {
			case '"': return new UnicodeString(parseStringLiteral());
			case '#': return ErrorConstant.valueOf(parseErrorLiteral());
			case 'F': case 'f':
			case 'T': case 't':
				return parseBooleanLiteral();
		}
		// else assume number
		return convertArrayNumber(parseNumber());
	}

