    /**
     * create a copy of an existing DoubleList
     *
     * @param list the existing DoubleList
     */

    public DoubleList(final DoubleList list)
    {
        this(list._array.length);
        System.arraycopy(list._array, 0, _array, 0, _array.length);
        _limit = list._limit;
    }

