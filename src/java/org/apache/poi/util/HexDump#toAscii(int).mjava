    public static char toAscii(int dataB) {
        char charB = (char)(dataB & 0xFF);
        if (Character.isISOControl(charB)) return '.';
        
        switch (charB) {
        case 0xFF: case 0xDD: // printable, but not compilable with current compiler encoding
            charB = '.';
            break;
        }
        return charB;
    }

