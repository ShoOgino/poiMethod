	/**
	   * @param format
	   * @return true if format is Unicode.
	   */
	  public static boolean isUnicodeFormat(final String format) {
	    try {
	      return !format.equals(new String(format.getBytes("ISO-8859-1"), "ISO-8859-1"));
	    } catch (UnsupportedEncodingException e) {
	      return true;
	    }
	  }

