    private static synchronized void initMsCodepointMap() {
        if (msCodepointToUnicode != null) return;
        msCodepointToUnicode = new HashMap<>();
        int i = 0xF020;
        for (int ch : symbolMap_f020) {
            msCodepointToUnicode.put(i++, ch);
        }
        i = 0xf0a0;
        for (int ch : symbolMap_f0a0) {
            msCodepointToUnicode.put(i++, ch);
        }
    }

