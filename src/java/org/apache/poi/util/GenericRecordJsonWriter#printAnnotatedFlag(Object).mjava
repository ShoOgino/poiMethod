    private void printAnnotatedFlag(Object o) {
        AnnotatedFlag af = (AnnotatedFlag) o;
        fw.write("0x");
        fw.write(Long.toHexString(af.getValue().get().longValue()));
        if (withComments) {
            fw.write(" /* ");
            fw.write(af.getDescription());
            fw.write(" */ ");
        }
    }

