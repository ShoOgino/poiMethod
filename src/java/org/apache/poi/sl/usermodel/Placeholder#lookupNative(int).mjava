    public static Placeholder lookupNative(int nativeId) {
        for (Placeholder ph : values()) {
            if (ph.nativeSlideId == nativeId ||
                ph.nativeSlideMasterId == nativeId || 
                ph.nativeNotesId == nativeId ||
                ph.nativeNotesMasterId == nativeId
            ) {
                return ph;
            }
        }
        return null;
    }

