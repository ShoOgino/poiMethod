    public Guide(String nm, String fm){
        name = nm;
        fmla = fm;
        operands = WHITESPACE.split(fm);
        switch (operands[0]) {
            case "*/": op = Op.muldiv; break;
            case "+-": op = Op.addsub; break;
            case "+/": op = Op.adddiv; break;
            case "?:": op = Op.ifelse; break;
            default: op = Op.valueOf(operands[0]); break;
        }
    }

