    public static synchronized PresetGeometries getInstance(){
        if(_inst == null) {
            _inst = new PresetGeometries();
            try {
                InputStream is = PresetGeometries.class.
                    getResourceAsStream("presetShapeDefinitions.xml");
                _inst.init(is);
                is.close();
            } catch (Exception e){
                throw new RuntimeException(e);
            }
        }

        return _inst;
    }

