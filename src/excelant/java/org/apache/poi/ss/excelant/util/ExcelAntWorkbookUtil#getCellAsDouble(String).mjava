    /**
     * Returns the value of the Cell as a double.
     * 
     * @param cellName
     * @return
     */
    public double getCellAsDouble( String cellName ) {
    	Cell cell = getCell( cellName ) ;
    	if( cell != null ) {
    		return cell.getNumericCellValue() ;
    	}
    	return 0.0 ;
    }

